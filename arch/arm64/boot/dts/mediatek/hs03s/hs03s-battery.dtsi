&pdc {
	pd_vbus_upper_bound = <10000000>;

	/* single charger */
	sc_input_current = <3200000>;
	sc_charger_current = <2700000>;

	/* dual charger in series */
	dcs_input_current = <2700000>;
	dcs_chg1_charger_current = <1300000>;
	dcs_chg2_charger_current = <1300000>;
