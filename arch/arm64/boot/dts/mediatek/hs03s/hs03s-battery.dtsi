	vbus_ctrl: vbus_ctrl {
		compatible = "hq, vbus_ctrl";
		vbus_ctrl_gpio_num = <171>;
		vbus_ctrl_gpio = <&pio 171 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <&vbus_ctrl_default>;
	};
	/*HS03s for SR-AL5625-01-250 by wenyaqi at 20210426 end*/
};
